label ccc4023:
    call gl(0,"bgcc0002")
    call vsp(0,1)
    with Dissolve(500.0/1000.0)
    "起きると、美希は腕の中にいた。"
    play bgm "bgm/bgm003.ogg"
    voice "vfCCC4023mki000"
    美希 "「ふわ……」"
    "目が合う。"
    voice "vfCCC4023mki001"
    美希 "「……きゃあ！？」"
    太一 "「おい！」"
    voice "vfCCC4023mki002"
    美希 "「え、あれ？　これって？」"
    "動揺しているではないか。"
    太一 "「エロいことをしたじゃないか」"
    voice "vfCCC4023mki003"
    美希 "「あ、そうか……そだそだ」"
    "舌を出す。"
    太一 "「おまえな……」"
    voice "vfCCC4023mki004"
    美希 "「くす……だってぇ、普段のセクハラのイメージしかないんですから」"
    太一 "「……うーん」"
    voice "vfCCC4023mki005"
    美希 "「あーあ、わたし処女じゃなくなっちゃった」"
    太一 "「うむ。もう立派なビッチだぞ」"
    voice "vfCCC4023mki006"
    美希 "「がぶ」"
    "鼻を噛まれた。"
    太一 "「いてえっ！」"
    voice "vfCCC4023mki007"
    美希 "「そんでちゅー！」"
    "飛びついてきた。"
    太一 "「おっと」"
    "真正面から抱き合って、チューをした。"
    voice "vfCCC4023mki008"
    美希 "「んん……んー……んんん……」"
    "朝の唾液交換。"
    "……勃った。"
    voice "vfCCC4023mki009"
    美希 "「う……こ、こりは？」"
    太一 "「いや」"
    voice "vfCCC4023mki010"
    美希 "「……します？」"
    太一 "「したいけど……時間平気か？」"
    voice "vfCCC4023mki011"
    美希 "「あー……」"
    voice "vfCCC4023mki012"
    美希 "「じゃあかわりにちゅー！」"
    "女の子はちゅーが好きなのか。"
    voice "vfCCC4023mki013"
    美希 "「んー」"
    "日曜は目覚ましを十時にセットしているのだが。"
    "丸々一時間、美希とちゅーをしていた。"
    pause (1000.0/1000.0)
    pause (500.0/1000.0)
    call gl(0,"bgcc0004")
    call vsp(0,1)
    with wipeleft
    call gl(1,"TCYM0000|TCYM0000")
    call gp(1,t=center)#x=180
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki014"
    美希 "「ちょーん」"
    "制服をキッチリ着こなした美希がいた。"
    call gl(1,"TCYM0011|TCYM0011")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki015"
    美希 "「すっきりさっぱりー」"
    太一 "「うむ」"
    call gl(1,"TCYM0003|TCYM0003")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki016"
    美希 "「太一くんもー、すっきりさっぱりー」"
    太一 "「うむー」"
    "二人で水風呂に入ったのだ。"
    "秘蔵の汲み置きだったのだが、どうせ今日で最後なら関係ない。"
    call gl(1,"TCYM0021|TCYM0021")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki017"
    美希 "「……なにしてるです？」"
    太一 "「ほれ」"
    call gl(1,"TCYM0004|TCYM0002")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki018"
    美希 "「あー、ひゃっこいビン！」"
    voice "vfCCC4023mki019"
    美希 "「え、どうやって？」"
    太一 "「風呂入ってる時間、小型冷蔵庫の急速モードで冷やした」"
    call gl(1,"TCYM0003|TCYM0003")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki020"
    美希 "「ビー玉の入ったラムネだぁ」"
    太一 "「田崎商店にて８０円」"
    voice "vfCCC4023mki021"
    美希 "「きれー」"
    太一 "「ビー玉を通して世界を見ると、きらきらと美しいのだ」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki022"
    美希 "「夏っぽいです」"
    太一 "「うむ」"
    "二人でラムネを飲んだ。"
    call gl(1,"TCYM0003|TCYM0003")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki023"
    美希 "「きれいになったし、ラムネもゴチになったし、そんじゃそろそろ行きますかねー」"
    太一 "「俺も部活に行こうっと」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki024"
    美希 "「……本当に部活に行くんですね」"
    太一 "「準備はできてるし」"
    太一 "「あの折れたアンテナでも、届かないことはないだろう」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki025"
    美希 "「あの」"
    voice "vfCCC4023mki026"
    美希 "「一緒に、同じ時間を生きませんか？」"
    "問いかける瞳。"
    "真剣に。"
    太一 "「……ありがたいけど」"
    voice "vfCCC4023mki027"
    美希 "「そうですか」"
    太一 "「忘れたいんだよ。なんか後味あまり良くないし」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki028"
    美希 "「……毎週、先輩を見てきました」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki029"
    美希 "「それぞれの先輩は、同一人物じゃないんですよ？」"
    太一 "「そうなるね」"
    voice "vfCCC4023mki030"
    美希 "「今生きて、話してるわたしたちは、今一度きりの自分なんです」"
    太一 "「でも美希。忘れてる」"
    太一 "「俺の理性は、すごく薄くて破れやすいものなんだ」"
    call gl(1,"TCYM0002|TCYM0002")
    call vsp(1,1)
    with dissolve
    voice "vfCCC4023mki031"
    美希 "「あ……」"
    太一 "「いつも一緒にいたら、美希が危ないよ」"
    太一 "「美希はすごく可愛いから、つい力が入ってしまうかも知れないし」"
    "頭を撫でる。"
    voice "vfCCC4023mki032"
    美希 "「……ままなりませんね」"
    太一 "「そういうものだよ」"
    太一 "「まあ甘えたかったら、いつでもその時々の黒須太一を頼ってくれ」"
    美希 "「…………」"
    "返事はなかった。"
    太一 "「さあ、行くか」"
    return
