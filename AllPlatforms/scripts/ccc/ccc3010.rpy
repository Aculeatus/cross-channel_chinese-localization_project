    call gl(1,"TCYM0003|TCYM0003")
    call gp(1,t=center)#x=180
    call vsp(1,1)
    with dissolve
    voice "vfCCC3010mki044"
    美希 "「まんぞくまんぞく」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3010mki045"
    美希 "「見ます？　戦果？」"
    太一 "「いいっす……」"
    太一 "「おお、よく聞こえる」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3010mki046"
    美希 "「すごく詰まってましたよ」"
    太一 "「別世界のようだ」"
    call gl(1,"TCYM0003|TCYM0003")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3010mki047"
    美希 "「わたしの耳かきで喜んでくれたのは、せんぱいがはじめてです」"
    "だろうな……。"
    "いや、決して喜んではいないが。"
    太一 "「むしろ俺の強力な煩悩力があったおかげで、耐えられたと言うべきか」"
    call gl(1,"TCYM0021|TCYM0021")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3010mki048"
    美希 "「ふお？」"
    太一 "「ふぉふぉふぉ」"
