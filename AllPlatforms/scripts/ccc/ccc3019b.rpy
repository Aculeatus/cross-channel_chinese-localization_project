label ccc3019b:
    call gl(0,"bgcc0010")
    call vsp(0,1)
    call vsp(1,0)
    with wipeleft
    "美希がいました。"
    太一 "「こらあ、そこの一年」"
    call gl(1,"TCYM0003|TCYM0003")
    call gp(1,t=center)#x=180
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki000"
    美希 "「あ、ちゃんと服着た先輩だ」"
    太一 "「ぐふっ」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki001"
    美希 "「もうチムチムは露出してないんですね……よかったぁ」"
    太一 "「チムチムのことは言うな」"
    太一 "「なぜ露出していたのか自分でもわからんのだ」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    美希 "「……………………」"
    "なぜ黙りこくるんだ。"
    太一 "「それよりこのダンボールは何かね？」"
    call gl(1,"TCYM0011|TCYM0011")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki003"
    美希 "「あー、これでもうひと遊びしようかと思ったんですが……バレちゃしょうがないですね」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki004"
    美希 "「これはミキハウス」"
    太一 "「……」"
    太一 "「いいのか？」"
    voice "vfCCC3019bmki005"
    美希 "「なにがでありましょーか？」"
    太一 "「いや……いろいろと……」"
    call gl(1,"TCYM0003|TCYM0003")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki006"
    美希 "「美希の家なのでミキハウス、なんの問題がありましょう」"
    "そう言われればそうか。"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki007"
    美希 "「敵地で発見されぬよう行動する時などに用います」"
    太一 "「無理だろう」"
    call gl(1,"TCYM0021|TCYM0021")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki008"
    美希 "「そうですか？」"
    太一 "「ミキハウスを見たら普通のヤングアダルトは『いい子供服あるかい？』って具合にダンディーに中をのぞきこみたくなるに決まっている」"
    call gl(1,"TCYM0041|TCYM0041")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki009"
    美希 "「……うう、わたしの身につけているものくらいしかお売り出来ないですね」"
    太一 "「いくらだ？」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki010"
    美希 "「え？」"
    太一 "「いくら出せばいい？」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki011"
    美希 "「どうしてそんな突然リアルな顔をして言うんですか？」"
    太一 "「リアル世代だからだ。だがそんなことはどうでもいい」"
    太一 "「いくらだ？」"
    call gl(1,"TCYM0021|TCYM0021")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki012"
    美希 "「んー、そーですねぇ」"
    "小悪魔みたいな、悪戯っぽい顔をしてみせる。"
    call gl(1,"TCYM0003|TCYM0003")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki013"
    美希 "「じゃあ一式百万円。即決価格。ただし今のみのタイムセールス。今お支払いできるなら、この場で脱いでお渡ししてもいいですよ。でもそれ以外はダメー」"
    "桜庭からもらった金貨を十枚渡した。"
    太一 "「買った。即決」"
    voice "vfCCC3019bmki014"
    美希 "「……………………えあ？」"
    "空気を意味する疑問語を発して美希の表情は凍結する。"
    太一 "「……この場で脱いでもらおう」"
    太一 "「着画がないのは残念だが、せめて脱衣シーンは撮影させてもうぞ」"
    "カメラを用意する。部室で良かった。"
    "バッテリーはまだ十分にあるな、よし。"
    call gl(1,"TCYM0004|TCYM0002")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki015"
    美希 "「…………あっ、あの……これって？」"
    太一 "「十万円金貨だ。美希は知るまいが」"
    太一 "「昭和の時代は戦争したり発展したり世界に嫌われたりしながら、６０年以上も続いたのだよ」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki016"
    美希 "「これって使えるんですか？」"
    太一 "「そこいらじゃ使えないが、まあ正式な通貨だぞ」"
    太一 "「さあ、即決した。入金も済んだ。次はそっちの番だ」"
    call gl(1,"TCYM0002|TCYM0002")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki017"
    美希 "「ひっ……」"
    "恐怖。"
    太一 "「もし約束を違えるのであれば……『たいへん悪い』という評価をつけるぞ」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki018"
    美希 "「女の子にとって、その噂はピンチ中のピンチです」"
    太一 "「なら脱ぎたまえ」"
    call gl(1,"TCYM0002|TCYM0002")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki019"
    美希 "「は、はわぁ、ふわ……そ、それは……」"
    call gl(2,"TCSK0001|TCSK000x")
    call gp(1,t=center)#x=0
    call gp(2,t=center)#x=340
    call vsp(2,1)
    call vsp(1,1)
    with Dissolve(500.0/1000.0)
    voice "vfCCC3019bkri000"
    霧 "「待って下さい！」"
    "霧だった。"
    太一 "「うっ、おまえは霧！」"
    "蹴られる。"
    "頭をガードする。"
    "が。"
    call gl(2,"TCSK0002|TCSK000x")
    call vsp(2,1)
    with dissolve
    voice "vfCCC3019bkri001"
    霧 "「……わたしが、かわりに脱ぎます」"
    call gl(1,"TCYM0004|TCYM0002")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki020"
    二人 "「えーーーーーーーーーーっ！？」"
    "霧はスカートの中に手を入れて、下着をずりおろす。"
    call gl(2,"TCSK0004|TCSK000x")
    call vsp(2,1)
    with dissolve
    voice "vfCCC3019bkri002"
    霧 "「ぐす……」"
    太一 "「ＯＫＯＫ、いいよいいよ！　やっぱなし！」"
    voice "vfCCC3019bkri003"
    霧 "「……いいんですか？」"
    太一 "「乙女を泣かせる趣味はない」"
    "せめて二人っきりなら……。"
    call gl(1,"TCYM0003|TCYM0003")
    call vsp(1,1)
    with dissolve
    voice "vfCCC3019bmki021"
    美希 "「ありがとー、霧ちー！」"
    call gl(2,"TCSK0006|TCSK000x")
    call vsp(2,1)
    with dissolve
    voice "vfCCC3019bkri004"
    霧 "「美希っ、よかった無事で」"
    call vsp(1,0)
    call vsp(2,0)
    with Dissolve(500.0/1000.0)
    "ひしっ"
    "抱きあう二人。"
    "……俺、悪者なんだね、この世界だと。"
    stop bgm
    return
