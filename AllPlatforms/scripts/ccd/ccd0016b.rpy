    call gl(1,"TCYM0004|TCYM0002")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001mki050"
    美希 "「うひゃ？」"
    "しかし本当に嫌がらんです、この人。"
    voice "vfCCD0001kri033"
    霧 "「……ぐぬぅ」"
    "霧からのオーラが、いよいよ物理的なものを帯びだした。"
    太一 "「俺、美希の魅力に感染しちゃったかも」"
    call gl(1,"TCYM0000|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001mki051"
    美希 "「あ、そ、そぉですか？」"
    太一 "「まぶたにゴミがついてるよ、目を閉じて」"
    call gl(1,"TCYM0001|TCYM0000")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001mki052"
    美希 "「……はい」"
    voice "vfCCD0001mki053"
    美希 "「どきどき」"
    "あ。唇盗れる。"
    "予定変更。"
    太一 "「ちゅー……」"
    call gl(2,"TCSK0001|TCSK000x")
