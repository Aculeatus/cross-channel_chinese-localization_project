    voice "vfCCD0001kri034"
    霧 "「それはさせない！！」"
    "飛びかかってきた。"
    太一 "「わっ、わっ？」"
    voice "vfCCD0001mki054"
    美希 "「きゃあ！？」"
    call gl(2,"TCSK0021|TCSK0021")
    call vsp(2,1)
    with dissolve
    voice "vfCCD0001kri035"
    霧 "「このチカンーっ！！」"
    太一 "「ぎゃーっ」"
    "寝取り失敗。"
    stop bgm
    call gl(0,"bgcc0000d")
    call vsp(0,1)
    call vsp(1,0)
    call vsp(2,0)
    with ImageDissolve("sys/EFMSK_12_MSK.bmp",time=1)
