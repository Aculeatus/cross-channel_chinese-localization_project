label ccd0004:
    call gl(0,"bgcc0005")
    call vsp(0,1)
    call vsp(1,0)
    with Dissolve(500.0/1000.0)
    play bgm "bgm/bgm006.ogg"
    "坂で少女とぶつかった。"
    call gl(1,"TCCN0102|TCCN")
    call gp(1,t=center)#x=180
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk000"
    七香 "「きゃ……いったたたぁ……どこ見て歩いてんのよー、このスカタン！」"
    太一 "「く……なんだよ、そっちがよそ見してるのが悪いんだろー！」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk001"
    七香 "「最初にぶつかった時から、好きだったの！」"
    "抱きついてくる。"
    太一 "「はええ！　展開はやすぎ！」"
    "こんな高速なステロみたことねー。"
    太一 "「……ぶっちゃけ、どちら様？」"
    voice "vfCCD0001nnk002"
    七香 "「七香」"
    太一 "「ななか……うん、いい名前だ」"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk003"
    七香 "「あんがと」"
    voice "vfCCD0001nnk004"
    七香 "「まああたしが何者かについては、続く調査を待つとして」"
    太一 "「幽霊じゃないの？」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk005"
    七香 "「あー、違うと思うわ」"
    太一 "「見た感じ、かなりゴースティックなんだが」"
    "※ゴースティック＝ゴースト的な"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk006"
    七香 "「違うわ。足あるし」"
    太一 "「いや……それは……最近のゴーストってけっこう足あるし……」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk004"
    七香 "「まああたしが何者かについては、続く調査を待つとして」"
    "強引に話そらされた。"
    太一 "「……いいけど」"
    voice "vfCCD0001nnk007"
    七香 "「じゃ名刺渡しておくから」"
    "受け取る。"
    "ＪＯＢ　次世代美少女"
    "ＮＡＭＥ　ななか～NANACA～"
    "あなたのハァト、癒してあ·げ·る"
    太一 "「…………」"
    "どこから突っ込もう？"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk008"
    七香 "「ふっふ～ん」"
    "七香とやらは誇らしげな、それこそ、"
    voice "vfCCD0001nnk009"
    "七香『惚れていいわよ』"
    "という不二子クラスの顔つきで俺に流し目を送ってきた。"
    太一 "「ななか、の綴り間違ってる」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk010"
    七香 "「わざとだよ」"
    "急にマジ顔。つうか怒ってる。"
    太一 "「…………」"
    "こいつ、やばい？"
    "敏腕ヤングアダルトとして名高い俺に、感じ取れない空気はない。"
    "告げていた。"
    "この女はヤバイと。"
    "だって、どう考えてもミスじゃん。ありえねー。"
    "だが……狂人相手にこれ以上つつくのは得策ではない。"
    太一 "「君って激キュートだけど、ご用件は？」"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk011"
    七香 "「いやー、さっきのは爽快だったよねー」"
    "一瞬で機嫌を直した。"
    "危険人物特有の反応だ。"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk012"
    七香 "「あの女の悶え苦しむ様！」"
    "クスクスと笑う。"
    太一 "「見てたのか」"
    voice "vfCCD0001nnk013"
    七香 "「ちらっとね」"
    voice "vfCCD0001nnk014"
    七香 "「スカッとした！」"
    太一 "「……曜子ちゃんのこと、嫌いなんだ？」"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk015"
    七香 "「いやな女。べーだよ、べー」"
    太一 "「君と曜子ちゃんの関係がわからん……」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk016"
    七香 "「ないよ。会ったことないし。ただムカつく」"
    voice "vfCCD0001nnk017"
    七香 "「だから今朝は気分いいんだー」"
    太一 "「あぁ、そうですか」"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk018"
    七香 "「だから今週は轢かないであげました」"
    "は、轢かない？"
    "意味わかんない。"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk019"
    七香 "「よくないよねー、あーいう人」"
    太一 "「……すっごい嫌いだってのは伝わってきた」"
    voice "vfCCD0001nnk020"
    七香 "「嫌いっていうか、間違ってるじゃない」"
    太一 "「まあ……」"
    "どこまで知ってるんだろう？"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk021"
    七香 "「あんな女に……太一の最初を取られるくらいだったら……あたしが……」"
    "暗くなった。"
    太一 "「最初？」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk022"
    七香 "「あー、やなこと思い出した。ペッペッ。忘れよ」"
    太一 "「あのう」"
    voice "vfCCD0001nnk023"
    七香 "「口直し」"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    "ちう"
    太一 "「…………っ！」"
    太一 "「な、なに、どうして！？」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk024"
    七香 "「いいじゃん、減るものじゃないし」"
    太一 "「なんか、妙に焦った……敏腕アダルトであるこの俺が……」"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk025"
    七香 "「ま、あんな女のことはどーでもいいや」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk026"
    七香 "「太一ぃ、くれぐれも惑わされないようにね？」"
    太一 "「そらまあ……されないと思うけどさ」"
    stop bgm
    "本当、なんなんだろう、この人。"
    "存在している密度がまるで感じられない。"
    "気配がないのだ。"
    call gl(1,"TCCN0102|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk027"
    七香 "「あのね、今日はちょっとマジでさ」"
    太一 "「はあ」"
    voice "vfCCD0001nnk028"
    七香 "「ちょっとこれから、あたしと一緒に祠に来て欲しいんだ」"
    太一 "「なんだ？　レイプか？」"
    voice "vfCCD0001nnk029"
    七香 "「するかっ！」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk030"
    七香 "「そーじゃなくて、真面目な話。祠、知ってるでしょ？」"
    play bgm "bgm/bgm018.ogg"
    太一 "「うん、まあ」"
    "あまり近寄りたい場所じゃないけど。"
    "なんせあそこは―――"
    call gl(1,"TCCN0102|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk031"
    七香 "「こーいうのは、ちょっと恐いんだけど……来てほしいんだ」"
    voice "vfCCD0001nnk032"
    七香 "「だって太一、あの女に比べて調査甘いんだもん」"
    太一 "「あの女って曜子ちゃん？」"
    call gl(1,"TCCN0101|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk033"
    七香 "「……悔しいけど優秀よね」"
    太一 "「脳みその使い方からして違うからね」"
    太一 "「あまり考えない方がいいよ、そこらへん」"
    call gl(1,"TCCN0100|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk034"
    七香 "「そうだね」"
    call gl(1,"TCCN0102|TCCN")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001nnk035"
    七香 "「ね、お願い！　これからちょっとつきあって！」"
    "手を合わされる。"
    return
    #