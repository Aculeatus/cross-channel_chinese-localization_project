label ccd0003:
    #modewin
    call gl(0,"bgcc0003")
    call vsp(0,1)
    with wipeleft
    call gl(0,"bgcc0002")
    call vsp(0,1)
    with ImageDissolve("sys/EFMSK_12_MSK.bmp",time=1)
    play bgm "bgm/bgm002.ogg"
    "学校に行こうと家を出ると、支倉曜子。"
    call gl(1,"TCHY0000|tchy000x")
    call gp(1,t=center)#x=180
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you000"
    曜子 "「おはよう、太一」"
    太一 "「うん。じゃ、そーいうことでばいばーい」"
    "立ち去る。"
    "背後から裾をつままれる。"
    太一 "「ぐえ……」"
    voice "vfCCD0001you001"
    曜子 "「ちょ、ちょっとくらい話してくれてもいいのに……」"
    "声が震えていた。"
    太一 "「キミは俺に従属的でありながら、たまに裏をかこうとする」"
    太一 "「そんなキミを全面的に信用することはできない」"
    call gl(1,"TCHY0001|tchy000x")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you002"
    曜子 "「適度な刺激を演出」"
    太一 "「俺を自分のてのひらで踊らせたいのだろうが、そうはイカ腹幼女」"
    voice "vfCCD0001you003"
    曜子 "「……島友貴っぽい。ちょっぴり幻滅」"
    "幻滅された。"
    call gl(1,"TCHY0000|tchy000x")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you004"
    曜子 "「いいじゃない。別に。太一、ちょっとサルっぽいし」"
    太一 "「馬鹿にしないでもらおう。俺は霊長類だ」"
    call gl(1,"TCHY0001|tchy000x")
    call vsp(1,1)
    with dissolve
    曜子 "「…………」"
    "馬鹿にされてる気がする。"
    "くそ、反抗的な。"
    "いじめてやる。"
    "スカートをめくった。"
    曜子 "「…………」"
    太一 "「ずいぶんセクシーなショーツをめしていらっしゃるじゃないか？」"
    call gl(1,"TCHY0000|tchy000x")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you007"
    曜子 "「女はいつも真剣勝負」"
    "動じない。"
    太一 "「パンツおろしますよ？」"
    voice "vfCCD0001you008"
    曜子 "「……するの？」"
    太一 "「しない……」"
    call gl(1,"TCHY0001|tchy000x")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you009"
    曜子 "「どうぞ」"
    "うー。"
    "つまらねー。"
    "この女には、フェチがない。"
    "恥じらいがない。"
    "ダメだ。"
    "彼女に俺のさくらんぼう魂を渡すわけにはいかない。"
    "肉体的にはともかくとして。"
    太一 "「じゃあこうだ」"
    "太一袋からソレを取り出す。"
    "そして彼女の鼻をつまむ。"
    "酸欠で口を開く瞬間を待った。"
    call gl(1,"TCHY0000|tchy000x")
    call vsp(1,1)
    with dissolve
    曜子 "「……」"
    "五秒。"
    "十秒。"
    "三十秒。"
    "一分。"
    "一分半。"
    太一 "「……あの、呼吸は？」"
    曜子 "「…………」"
    "なんで平気なんですか？"
    "やめた。"
    "手を離すと、彼女は軽く息を整えた。"
    call gl(1,"TCHY0001|tchy000x")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you012"
    曜子 "「血中の酸素量が多いと、長く止めていられる」"
    太一 "「海人さんか、きみは」"
    call gl(1,"TCHY0000|tchy000x")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you013"
    曜子 "「なにがしたかったの？」"
    太一 "「……口を開けさせようかなって思って」"
    voice "vfCCD0001you014"
    曜子 "「あーん」"
    "簡単だった。"
    太一 "「ベロ出して」"
    voice "vfCCD0001you015"
    曜子 "「んー」"
    "長いベロ。"
    "タバスコを振る。十滴くらい。"
    "指で塗りたくる。"
    "やーこいベロ。"
    "それに心なしか表面がざらざらしてる。"
    "最高のピンサロ嬢になる素質を持っていた。"
    太一 "「はい、終わりました」"
    call gl(1,"TCHY0001|tchy000x")
    call vsp(1,1)
    with dissolve
    曜子 "「……」"
    "一瞬平気なのかな、と思ったが。"
    voice "vfCCD0001you017"
    曜子 "「……っっ」"
    "口を押さえて震えだした。"
    "煉瓦の壁によりかかってしまう。"
    "効いた。"
    "常人より秀逸な舌を持っているせいで、敏感なのだ。"
    太一 "「ジュース飲む？」"
    voice "vfCCD0001you018"
    曜子 "「……うん、うん……はやく……」"
    "もぎ取るようにして飲む。"
    太一 "「チリジュースおいしい？」"
    voice "vfCCD0001you019"
    曜子 "「う……」"
    hide pic1
    with dissolve
    "垣根に向かって倒れた。"
    太一 "「勝った……」"
    "久々にまともに勝った気がする。"
    "さて、学校に行こう。"
    call gl(1,"TCHY0002|tchy000x")
    call gp(1,t=center)
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you020"
    曜子 "「……せめ……て……おべ……んと……」"
    "しがみついてくる。"
    "涙目で紙袋を渡そうとしてくる。"
    "多少は罪悪感も刺激された。"
    太一 "「……わかった、もらうよ」"
    "母親ばりだ。"
    "母親いないけど。"
    call gl(1,"TCHY0000|tchy000x")
    call vsp(1,1)
    with dissolve
    voice "vfCCD0001you021"
    曜子 "「いってらっしゃい」"
    "ありがたいけど、素直に喜べなかった。"
    stop bgm
    return
    #